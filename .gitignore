cat << 'EOF' > .gitignore
# Byte-compiled / optimized / DLL files
__pycache__/
*.py[cod]
*.so
*.pyd
*.pyo

# Virtual environment
venv/
env/
.venv/

# System files
.DS_Store
Thumbs.db

# VSCode settings
.vscode/

# Streamlit cache
.streamlit/config.toml
.streamlit/*.json
.streamlit/*.py

# Logs and debug
*.log

# API keys 
.env

# PyInstaller/Packaging stuff
dist/
build/
*.spec
EOF
